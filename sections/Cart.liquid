{% if cart.item_count > 0 %}
<script>
  var checkoutBar = document.getElementById('checkout-bar');
  if(!checkoutBar) {
// create checkoutbar 
var checkoutbar = document.createElement('div');
checkoutbar.id = 'checkout-bar';
checkoutbar.classList.add('checkout-bar');
// create button 
var checkoutbarbutton = document.createElement('a');
checkoutbarbutton.id = 'checkout-bar-button';
checkoutbarbutton.setAttribute('href','#');
// create total
var checkoutbartotal = document.createElement('span');
checkoutbartotal.id = 'checkout-bar-total';
// put continue and total into button
checkoutbarbutton.innerHTML = 'Continue';
checkoutbarbutton.append(checkoutbartotal);
checkoutbar.innerHTML = '<a href="#" onclick="goBack()" class="continue"> <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="arrow-left" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512" class="svg-inline--fa fa-arrow-left fa-w-14 fa-3x" style=" width: 10px; margin-right: 3px;"><path fill="currentColor" d="M257.5 445.1l-22.2 22.2c-9.4 9.4-24.6 9.4-33.9 0L7 273c-9.4-9.4-9.4-24.6 0-33.9L201.4 44.7c9.4-9.4 24.6-9.4 33.9 0l22.2 22.2c9.5 9.5 9.3 25-.4 34.3L136.6 216H424c13.3 0 24 10.7 24 24v32c0 13.3-10.7 24-24 24H136.6l120.5 114.8c9.8 9.3 10 24.8.4 34.3z" class=""></path></svg>Continue Shopping</a>';    
checkoutbar.append(checkoutbarbutton);
document.body.append(checkoutbar);
  }
</script>

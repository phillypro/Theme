<script>
  
var getNextSibling = function (elem, selector) {

	// Get the next sibling element
	var sibling = elem.nextElementSibling;

	// If there's no selector, return the first sibling
	if (!selector) return sibling;

	// If the sibling matches our selector, use it
	// If not, jump to the next sibling and continue the loop
	while (sibling) {
		if (sibling.matches(selector)) return sibling;
		sibling = sibling.nextElementSibling
	}

};
var getPreviousSibling = function (elem, selector) {

	// Get the next sibling element
	var sibling = elem.previousElementSibling;

	// If there's no selector, return the first sibling
	if (!selector) return sibling;

	// If the sibling matches our selector, use it
	// If not, jump to the next sibling and continue the loop
	while (sibling) {
		if (sibling.matches(selector)) return sibling;
		sibling = sibling.previousElementSibling;
	}

};  
  
var shareButtons = document.querySelectorAll('.yotpo .share-button');  
  shareButtons.forEach(function(shareButton) {
    shareButton.addEventListener('click', sharelinks);
  });  
  
function sharelinks(event) {
event.preventDefault();
var shareButtonContainer = event.currentTarget.parentElement;
  if(shareButtonContainer.classList.contains('open')) {
   shareButtonContainer.classList.remove('open');
  }else{
   shareButtonContainer.classList.add('open');
  }
}   

var writeLinks = document.querySelectorAll('.yotpo .write a');  
  writeLinks.forEach(function(writeLink) {
    writeLink.addEventListener('click', trywritelink);
  });  
  
function trywritelink(event) {
event.preventDefault();
var writeLinkContainer = event.currentTarget.parentElement;
  if(writeLinkContainer.classList.contains('open')) {
   setTimeout(function() {
   writeLinkContainer.classList.remove('open');
   }, 2000);
  }else{
   writeLinkContainer.classList.add('open');
  }
}     

	  
var yotpoHelpfuls = document.querySelectorAll('.yotpo .yotpo-helpful a');  
  yotpoHelpfuls.forEach(function(yotpoHelpful) {
    console.log(yotpoHelpful);
    yotpoHelpful.addEventListener('click', yotpovote);
  });  
  
function yotpovote(event) {
  event.preventDefault();
  var countdiv = event.currentTarget.querySelector('span');
  var count = countdiv.innerHTML; 
  var siblingParent = getNextSibling(event.currentTarget, 'a') ? getNextSibling(event.currentTarget, 'a') : getPreviousSibling(event.currentTarget, 'a');
  var sibling = siblingParent.querySelector('span');
		if(count > 0) {
			countdiv.innerHTML = '0';
			sibling.innerHTML = '0';
		}else{
			countdiv.innerHTML = '1';
			sibling.innerHTML = '0';
		}
}   

 var verifiedChecks = document.querySelectorAll('.yotpo .verified-check');  
  verifiedChecks.forEach(function(verifiedCheck) {
    verifiedCheck.addEventListener('mouseenter', verifiedHover);
    verifiedCheck.addEventListener('mouseleave', verifiedHoverOut);
  });   
 
function verifiedHover(event) {
  console.log(event.target);
  event.target.parentElement;
  var headerElement = getNextSibling(event.target.parentElement, '.yotpo-header-element');
  headerElement.querySelector('.verified').classList.add('hovered');
}
function verifiedHoverOut(event) {
  console.log(event.target);
  event.target.parentElement;
  var headerElement = getNextSibling(event.target.parentElement, '.yotpo-header-element');
  headerElement.querySelector('.verified').classList.remove('hovered');
}
  
  
	  
	/* 
	jQuery('.yotpo-helpful a').click(function(e) {
		var countdiv = jQuery(this).children('span');
		var count = countdiv.text();
		var sibling = jQuery(this).siblings().children('span');
		if(count > 0) {
			countdiv.text('0');
			sibling.text('0');
		}else{
			countdiv.text('1');
			sibling.text('0');
		}
	});
	    
*/
</script>








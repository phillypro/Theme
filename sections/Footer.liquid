{% assign loadedfonts = '' %}    
{% style %}
  #shopify-section-Footer {
  {% capture backgroundimage %}{{ section.settings.top_background_image | img_url: '1024x1024' }}{% endcapture %}          
{% if backgroundimage contains 'no-image' %}
{% if section.settings.top_background_color_2 contains 'transparent' %}
      background-color:{{ section.settings.top_background_color }};                            
  {% else %}     
    background: -moz-linear-gradient(left, {{ section.settings.top_background_color }} 0%, {{ section.settings.top_background_color_2 }} 100%);
    background: -webkit-linear-gradient(left, {{ section.settings.btop_ackground_color }} 0%, {{ section.settings.top_background_color_2 }} 100%);
    background: linear-gradient(to right, {{ section.settings.top_background_color }} 0%, {{ section.settings.top_background_color_2 }} 100%);                            
{% endif %}  
{% else %}
background-image:url({{ section.settings.top_background_image | img_url: '1024x1024' }}); background-color:{{ section.settings.top_background_color }};
{% endif %}     
  }
  
  #shopify-section-Footer .bottom-footer {
  {% if section.settings.bottom_background_color_2 contains 'transparent' or section.settings.bottom_background_color_2 contains 'rgba(0,0,0,0)' %}
      background-color:{{ section.settings.bottom_background_color }};                            
  {% else %}     
    background: -moz-linear-gradient(left, {{ section.settings.bottom_background_color }} 0%, {{ section.settings.bottom_background_color_2 }} 100%);
    background: -webkit-linear-gradient(left, {{ section.settings.bottom_background_color }} 0%, {{ section.settings.bottom_background_color_2 }} 100%);
    background: linear-gradient(to right, {{ section.settings.bottom_background_color }} 0%, {{ section.settings.bottom_background_color_2 }} 100%);                            
{% endif %}  
    color:{{ section.settings.bottomcolor }};
  }
  #shopify-section-Footer .bottom-footer a, #shopify-section-Footer .bottom-footer svg.original path {
    color:{{ section.settings.bottomcolor }};
    fill:{{ section.settings.bottomcolor }};
  }


  
  #shopify-section-Footer .middle-footer .card-icons svg path {
    fill:{{ section.settings.card_color }};
  }
  
{% if section.settings.css != nil %}
{% assign css = section.settings.css | replace: '&gt;','>' %}
{{ css }}
{% endif %}
{% endstyle %}


  
<div class="middle-footer"> 
{% for block in section.blocks %}
  {% case block.type %}
    {% when 'HTML' %}
     {{ block.settings.html }}
    {% when 'Mailing-List' %}
    {% style %}      
      
{% assign loadedfonts = '' %}    
{% assign familyandweight = block.settings.font.family | append: block.settings.font.weight %}
{% if loadedfonts contains familyandweight %}
{% else %}
  {{ block.settings.font | font_face }}  
{% endif %}   
{% assign loadedfonts = loadedfonts | append: block.settings.font.family | append: blocksettings.font.weight | append: block.settings.font.style | append: '|' %}

  
  
      .mailing-list-wrap.mail-{{ block.id }} {
        margin: {{ block.settings.malingListtopposition }}px auto {{ block.settings.malingListbottomposition }}px ;
      }
      .mailing-list-wrap.mail-{{ block.id }} p {
      color:{{ block.settings.message_color }};
      font-size:{{ block.settings.fontsize }}px;
            font-family: {{ block.settings.font.family }}; 
    font-weight:{{ block.settings.font.weight }}; 
    font-style:{{ block.settings.font.style }};  
      }
     .mailing-list-wrap.mail-{{ block.id }} form button[type=submit] {
      background:{{ block.settings.button_color }};color:{{ block.settings.button_font_color }};
      }
    {% endstyle %}
    <div class="mailing-list-wrap mail-{{ block.id }}">
       <div class="mailing-list-inner">
     {% if block.settings.message %}
         <p><span>{{ block.settings.message }}</span></p>
     {% endif %}
     {% if block.settings.mailchimp_url %}
              {% assign mailchimpform = block.settings.mailchimp_url | split: 'u=' %}
              {% assign mailchimpform = mailchimpform[1] %}
              {% assign mailchimpformids = mailchimpform | split: '&amp;id=' %}
             <!-- Begin Mailchimp Signup Form -->

          
     <div id="mc_embed_signup">    
     <form id="mc-embedded-subscribe-form-Footer" name="mc-embedded-subscribe-form" class="validate" action="{{ block.settings.mailchimp_url }}"  method="post" target="_blank" novalidate>
      <input class="email" type="email" value="" name="EMAIL" id="mce-EMAIL" placeholder="{{ block.settings.input_text }}">
      <div style="position: absolute; left: -5000px;" aria-hidden="true">
          <input type="text" name="b_{{ mailchimpformids[0] }}_{{ mailchimpformids[1] }}" tabindex="-1" value="">
      </div>
       
       
      <button name="subscribe" id="mc-embedded-subscribe" type="submit" class="mc-button">{{ block.settings.button_text }}</button>

     </form>  

		<div class="response" id="mce-error-response" style="display:none;"></div>
     </div>  
<!--End mc_embed_signup-->
       {% else %}     
     <form id="klaviyoFooter" class="klaviyoform" action="//manage.kmail-lists.com/subscriptions/subscribe" data-ajax-submit="//manage.kmail-lists.com/ajax/subscriptions/subscribe" method="GET" target="_blank" novalidate="novalidate">
      <input type="hidden" name="g" value="{{ block.settings.klaviyo_list }}">
      <input type="email" value="" name="email" id="k_id_email" placeholder="{{ block.settings.input_text }}">
      <button type="submit" class="klaviyo_submit_button">{{ block.settings.button_text }}</button>
     </form>
      {% endif %}
       </div>
    </div>
    {% assign ajax = block.settings.ajax %}
 
  {% unless mailchimpform %}   
  <script>
var callback{{ section.id }}= function(){
  // Handler when the DOM is fully loaded
  if(!window.KlaviyoSubscribe) {
    // add script
    var script = document.createElement("script");
    script.type = "text/javascript";   
    script.src = 'https://www.klaviyo.com/media/js/public/klaviyo_subscribe.js';
    document.getElementsByTagName("head")[0].appendChild(script);
  }
};

if (
    document.readyState === "complete" ||
    (document.readyState !== "loading" && !document.documentElement.doScroll)
) {
  callback{{ section.id }}();
} else {
  document.addEventListener("DOMContentLoaded", callback{{ section.id }});
}   
</script> 
{% endunless %}  
 
  
   {% when 'Social-Icons' %}
  {% style %}
    #shopify-section-Footer .middle-footer .social-icons svg path {
    fill:{{ block.settings.social_color }};
  }
  {% endstyle %}
